namespace SiliconStudio.Paradox.Graphics.Tests
{
    class MyCustomColorTransformShader : ColorTransformShader
    {
        [Color]
        float4 Color;

        override float4 Compute(float4 color)
        {
            return float4(color.rgb + Color.rgb, color.a);
        }
    };
}